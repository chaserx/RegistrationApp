<h1>Symposium Registrations</h1>

<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>Title</th>
    <th>Firstname</th>
    <th>Lastname</th>
    <th>Organization</th>
    <th>Address1</th>
    <th>Address2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Status</th>
    <th>Dept</th>
    <th>Eveningsession</th>
    <th>Guest</th>
    <th>Partysize</th>
    <th>Lunch</th>
    <th>Bizperson</th>
    <th>Bizpersonemail</th>
    <th>Bizpersonphone</th>
    <th>Fees</th>
    <th>Abstracttext</th>
    <th>Abstracttitle</th>
    <th>Abstractauthors</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @regs.each do |reg| %>
  <tr>
    <td><%= reg.title %></td>
    <td><%= reg.firstname %></td>
    <td><%= reg.lastname %></td>
    <td><%= reg.organization %></td>
    <td><%= reg.address1 %></td>
    <td><%= reg.address2 %></td>
    <td><%= reg.city %></td>
    <td><%= reg.state %></td>
    <td><%= reg.zip %></td>
    <td><%= reg.phone %></td>
    <td><%= reg.email %></td>
    <td><%= reg.status %></td>
    <td><%= reg.dept %></td>
    <td><%= reg.eveningsession %></td>
    <td><%= reg.guest %></td>
    <td><%= reg.partysize %></td>
    <td><%= reg.lunch %></td>
    <td><%= reg.bizperson %></td>
    <td><%= reg.bizpersonemail %></td>
    <td><%= reg.bizpersonphone %></td>
    <td><%= reg.fees %></td>
    <td><%= reg.abstracttext %></td>
    <td><%= reg.abstracttitle %></td>
    <td><%= reg.abstractauthors %></td>
    <td><%= link_to 'Show', reg %></td>
    <td><%= link_to 'Edit', edit_reg_path(reg) %></td>
    <td><%= link_to 'Destroy', reg, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />
